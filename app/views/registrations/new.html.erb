<%= form_for @account, url: signup_path do |f| %>
	<%= f.label :username %>
	<%= f.text_field :username %>
	<%= f.label :email %>
	<%= f.email_field :email %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.label :password_confirmation %>	
	<%= f.password_field :password_confirmation %>
	<%= f.submit %>
<% end %>